PREFIX xsd: <http://www.w4.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX dbp: <http://dbpedia.org/ontology/>
SELECT *
WHERE {
        dbp:subOfAthlete rdfs:subClassOf dbp:Athlete .
        dbp:subOfAthlete rdfs:label dbp:athleteGroup .
        ?athlete a dbp:subOfAthlete .
        ?athlete dbp:birthDate ?birth .
        ?athlete dbp:deathDate ?death .
}